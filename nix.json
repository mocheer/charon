{
  "name":"charon",
  "scripts":{
    "dev":"air",
    "build":"go build -ldflags \"-s -w\"",
    "build-dist":"go build -ldflags \"-s -w\" -o dist/{appName}.exe",
    "install":"nix nssm install {appName} {execPath}/charon.exe",
    "start":"nix nssm start {appName}",
    "stop":"nix nssm stop {appName}",
    "remove":"nix nssm remove {appName}",
    "setup":"nix run install && nix run start",
    "unsetup":"nix run stop && nix run remove"
  }
}